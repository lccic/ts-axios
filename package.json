{
  "name": "ts-axios-lccic",
  "version": "1.0.1",
  "description": "use typescript implement axios",
  "keywords": ["use typescript implement axios"],
  "main": "dist/ts-axios.umd.js",
  "module": "dist/ts-axios.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "lccic <1491483064@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/lccic/ts-axios"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "dev": "node examples/server.js",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs  --theme minimal --entryPoints src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "prepub": "npm run test:prod && npm run build",
    "pub": "sh release.sh"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "/test/.*\\.(test|spec)\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}",
      "src/**/*.{js,ts}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/boot.ts"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jasmine-ajax": "^3.1.39",
    "@types/jest": "^24.0.13",
    "@types/node": "^16.3.2",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.5.3",
    "atob": "^2.1.2",
    "body-parser": "^1.18.3",
    "colors": "^1.3.2",
    "commitizen": "^4.2.4",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "^1.4.4",
    "coveralls": "^3.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "cz-conventional-changelog": "^3.3.0",
    "express": "^4.16.4",
    "husky": "^7.0.1",
    "jasmine-ajax": "^4.0.0",
    "jasmine-core": "^3.4.0",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "lint-staged": "^11.0.1",
    "lodash.camelcase": "^4.3.0",
    "nprogress": "^0.2.0",
    "prettier": "^2.3.2",
    "prompt": "^1.0.0",
    "qs": "^6.7.0",
    "replace-in-file": "^6.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "semantic-release": "^17.4.4",
    "shelljs": "^0.8.3",
    "style-loader": "^3.1.0",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^24.0.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
